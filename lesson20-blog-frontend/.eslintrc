{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": ["react", "jsx-a11y", "import"],
    "rules": {
        "react/jsx-filename-extension": 0,
        "linebreak-style": 0,
        "import/no-unresolved": 0,
        "no-use-before-define": 0,
        "no-param-reassign": 0
    },
    "env": {
        "browser": true,
        "node": true
    }
}